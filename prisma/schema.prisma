datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider        = "prisma-client-js"
  }



model gallery {
  id    Int     @id @default(autoincrement())
  thumbnailimg String  
  image  String?
  video String?
  status Int
  category Int
}

model categories {
  id    Int     @id @default(autoincrement())
  status Int
  category String
}